{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "import/prefer-default-export": "off",
        "import/no-unresolved": [
            0, 
            { "caseSensitive": false }
        ],
        "no-useless-constructor": "off",
        "no-unused-vars": "off",
        "no-empty-function": "off",
        "class-methods-use-this": "off",
        "import/extensions": "off"
    }
}
